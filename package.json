{
  "name": "gupil",
  "version": "1.0.0",
  "private": true,
  "description": "Gupil - SaaS платформа бонусных программ с Telegram ботами",
  "author": {
    "name": "Kiran",
    "url": "https://github.com/Kiranism"
  },
  "scripts": {
    "dev": "next dev --turbopack -p 5006",
    "build": "next build",
    "start": "next start -H 0.0.0.0 -p 3000",
    "docs:dev": "cd user-docs && yarn install && yarn dev",
    "docs:build": "cd user-docs && yarn install && yarn build",
    "docs:export": "cd user-docs && yarn install && yarn export",
    "lint": "node scripts/run-eslint.mjs src",
    "lint:fix": "eslint src --fix && yarn format",
    "lint:strict": "eslint --max-warnings=0 src",
    "format": "prettier --write .",
    "format:check": "prettier -c -w .",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=services",
    "test:integration": "jest --testPathPattern=api",
    "test:components": "jest --testPathPattern=components",
    "test:webhook": "tsx scripts/test-webhook-integration.ts",
    "tunnel": "ngrok http 5006",
    "deploy:vercel": "vercel --prod",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:migrate": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:studio": "prisma studio --browser none --port 5555",
    "production:check": "yarn lint && yarn test && yarn build && npx tsc --noEmit",
    "pm2:start": "pm2 start ecosystem.config.cjs --update-env --only bonus-app && pm2 save",
    "pm2:restart": "pm2 restart bonus-app --update-env",
    "pm2:stop": "pm2 delete bonus-app || true",
    "prod:up": "yarn build && yarn pm2:start",
    "migrate-customers": "tsx scripts/migrate-airtable-customers.ts",
    "migrate-owners": "tsx scripts/migrate-project-owners.ts",
    "check-migration": "tsx scripts/check-migration.ts",
    "test-api": "tsx scripts/test-api.ts",
    "update-levels": "tsx scripts/update-user-levels.ts",
    "validate-migration": "tsx scripts/validate-migration.ts",
    "rollback-migration": "tsx scripts/rollback-migration.ts",
    "fix-bonus-behavior": "tsx scripts/fix-bonus-behavior-direct.ts",
    "import-users": "tsx scripts/import-users-from-csv.ts"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ]
  },
